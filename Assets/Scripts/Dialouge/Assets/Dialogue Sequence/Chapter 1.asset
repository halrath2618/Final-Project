      text: 
      nextId: 
    choice:
      options:
      - text: "T\xF4i\u2026 s\u1EE3. \u1EDE l\u1EA1i \u0111\xE2y."
        nextId: n34_gameover1
      - text: "T\xF4i s\u1EBD kh\xF4ng b\u1ECF cu\u1ED9c. T\xF4i ph\u1EA3i tr\u1EDF
          v\u1EC1."
        nextId: n40_goon
  - id: n34_gameover1
    type: 0
